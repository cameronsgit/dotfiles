{
    "atlascode.jira.workingSite": {
        "baseUrlSuffix": "atlassian.net"
    },
    "window.nativeTabs": true,
    "breadcrumbs.enabled": false,
    "editor.autoClosingBrackets": "never",
    "editor.autoSurround": "never",
    "editor.autoClosingQuotes": "never",
    "editor.fontFamily": "Operator Mono for Powerline",
    "editor.fontSize": 13,
    "editor.minimap.enabled": false,
    "editor.renderIndentGuides": true,
    "editor.renderWhitespace": "boundary",
    "editor.snippetSuggestions": "none",
    "editor.wordWrap": "on",
    "explorer.decorations.colors": false,
    "explorer.openEditors.visible": 0,
    "files.exclude": {
        "**/.git": false
    },
    "powershell.integratedConsole.showOnStartup": false,
    "projectManager.git.baseFolders": [
        "$home/Development"
    ],
    "workbench.colorTheme": "Gruvbox Dark Medium",
    "workbench.editor.tabCloseButton": "left",
    "workbench.iconTheme": "vscode-icons",
    "workbench.sideBar.location": "left",
    "workbench.statusBar.visible": true,
    "yaml.schemas": {
        "file:///Users/sowderca/.vscode/extensions/atlassian.atlascode-1.4.3/resources/schemas/pipelines-schema.json": "bitbucket-pipelines.yml"
    },
    "team.showWelcomeMessage": false,
    "vim.enableNeovim": true,
    "vim.neovimPath": "/usr/local/bin/nvim",
    "vim.useSystemClipboard": true,
    "powershell.powerShellExePath": "/usr/local/bin/pwsh-preview",
    "team.pinnedQueries": [
        {
            "account": "dev.azure.com/blackbaud",
            "queryText": "select * from WorkItems where [System.AssignedTo] = @me and [System.State] not in ('Closed')"
        },
        {
            "account": "dev.azure.com/blackbaud",
            "queryPath": "My Queries/WIP"
        }
    ],
    "editor.suggestSelection": "first",
    "typescript.updateImportsOnFileMove.enabled": "always",
    "java.configuration.checkProjectSettingsExclusions": false,
    "todohighlight.defaultStyle": {
        "backgroundColor": "#282828"
    },
