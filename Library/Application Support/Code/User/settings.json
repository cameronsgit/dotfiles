    "workbench.colorCustomizations": {
        "[Default Dark+]": {
            "editor.background": "#282828"
        }
    },
    "editor.tokenColorCustomizations": {
        "[Default Dark+]": {
            "textMateRules": [
                {
                    "scope": "comment",
                    "settings": {
                        "fontStyle": "italic",
                        "foreground": "#928374"
                    }
                }
            ]
        },
        "[Gruvbox Dark Soft]": {
            "textMateRules": [
                {
                "scope": "support.class.dart",
                "settings": {
                    "foreground": "#fabd2f"
                }
            },
            {
                "scope": [
                    "variable.other.readwrite.cs",
                    "entity.name.variable.local.cs",
                    "entity.name.variable.property.cs",
                    "entity.name.variable.parameter.cs",
                    "entity.name.type.namespace.cs"
                ],
                "settings": {
                    //"foreground": "#83a598",
                    "foreground": "#458588"
                }
            },
            {
                "scope": [
                    // "meta.function.cs",
                    // "entity.name.function.cs",
                    // "variable.other.object.cs",
                    "keyword.control",
                    "storage.modifier.cs",
                    "keyword.other.await.cs"
                ],
                "settings": {
                    "foreground":  "#e2484d", 
                }
            },
          {
                "scope": [
                    // "meta.function.cs",
                    // "entity.name.function.cs",
                    // "variable.other.object.cs",
                ],
                "settings": {
                    "foreground": "#d79921",
                }
            },
            {
                "scope": [
                    // "variable.other.object.cs",
                    "keyword.operator.logical.cs",
                ],
                "settings": {
                    "foreground": "#aec07c",
                }
            },
            {
                "scope": [
                    "keyword.type"
                ],
                "settings": {
                    "foreground": "#fabd2f",
                }
            },
            {
                "scope": [
                    "variable.other.member.cs",
                    "meta.function.cs",
                    "entity.name.function.cs",
                ],
                "settings": {
                    // "foreground": "#fe8019",
                    "foreground": "#d3896b" 
                }
            },
            {
                "scope": [
                    "keyword.cs",
                    "storage.type.powershell"
                ],
                "settings": {
                    "foreground": "#e95c4c",
                }
            },
            {
                "scope": [
                    "variable.other.object.cs",
                    "variable.other.object.property.cs",
                ],
                "settings": {
                    // "foreground": "#458588",
                    "foreground": "#83a598"
                }
            },
            {
                "scope": [
                    "meta.function.cs",
                    "entity.name.function.cs",
                    //"variable.other.object.cs",
                ],
                "settings": {
                    // "foreground": "#d3896b"
                    "foreground": "#fe8019"
                }
            }
        ]
        }
    },
