{
    "emmet.excludeLanguages": [
        "*"
    ],
    "atlascode.jira.workingSite": {
        "baseUrlSuffix": "atlassian.net"
    },
    "window.nativeTabs": true,
    "breadcrumbs.enabled": false,
    "editor.autoClosingBrackets": "never",
    "editor.autoSurround": "never",
    "editor.autoClosingQuotes": "never",
    "editor.fontFamily": "Operator Mono for Powerline",
    "editor.fontSize": 13,
    "editor.minimap.enabled": false,
    "editor.renderIndentGuides": true,
    "editor.renderWhitespace": "boundary",
    "editor.snippetSuggestions": "none",
    "editor.wordWrap": "on",
    "explorer.decorations.colors": false,
    "explorer.openEditors.visible": 0,
    "files.exclude": {
        "**/.git": false
    },
    "powershell.integratedConsole.showOnStartup": false,
    "projectManager.git.baseFolders": [
        "$home/Development"
    ],
    "workbench.editor.tabCloseButton": "left",
    "workbench.iconTheme": "vscode-icons",
    "workbench.sideBar.location": "left",
    "workbench.statusBar.visible": true,
    "yaml.schemas": {
        "file:///Users/sowderca/.vscode/extensions/atlassian.atlascode-1.4.3/resources/schemas/pipelines-schema.json": "bitbucket-pipelines.yml"
    },
    "team.showWelcomeMessage": false,
    "vim.enableNeovim": true,
    "vsicons.associations.folders": [
        { "icon": "temp", "extensions": ["obj"], "format": "svg" }
    ],
    "vim.neovimPath": "/usr/local/bin/nvim",
    "vim.useSystemClipboard": true,
    "powershell.powerShellExePath": "/usr/local/bin/pwsh-preview",
    "team.pinnedQueries": [
        {
            "account": "global",
            "queryText": "select * from WorkItems where [System.AssignedTo] = @me and [System.State] not in ('Closed')"
        }
    ],
    "editor.suggestSelection": "first",
    "typescript.updateImportsOnFileMove.enabled": "always",
    "java.configuration.checkProjectSettingsExclusions": false,
    "todohighlight.defaultStyle": {
        "backgroundColor": "#282828"
    },
    "todohighlight.keywords": [
        {
            "text": "FIXME",
            "color": "pink"
        }
    ],
    "files.associations": {
        "*.tf": "terraform",
        "*.command": "shellscript",
        "*.plist": "xml",
        "Vagrantfile": "ruby"
    },
    "mssql.intelliSense.lowerCaseSuggestions": true,
    "pgsql.intelliSense.lowerCaseSuggestions": true,
    "pgsql.format.keywordCasing": "lowercase",
    "python.jediEnabled": false,
    "atlascode.jira.statusbar.showLogin": false,
    "pgsql.format.datatypeCasing": "lowercase",
    "mssql.format.keywordCasing": "lowercase",
    "mssql.format.datatypeCasing": "lowercase",
    "window.zoomLevel": 0,
    "go.vetOnSave": "off",
    "go.testOnSave": false,
    "go.coverOnSave": false,
    "go.lintOnSave": "off",
    "go.formatTool": "gofmt",
    "[go]": {
        "editor.formatOnSave": false
    },
    "go.buildOnSave": "off",
    "powershell.codeFormatting.useCorrectCasing": true,
    "emmet.showExpandedAbbreviation": "never",
    "workbench.colorTheme": "Gruvbox Dark Medium",
    "javascript.suggest.autoImports": false,
    "python.terminal.activateEnvironment": false,
    "terraform.indexing": {
        "enabled": false,
        "liveIndexing": false,
        "delay": 500,
        "exclude": [
            ".terraform/**/*",
            "**/.terraform/**/*"
        ]
    },
    "terraform.languageServer": {
        "enabled": true,
        "args": []
    }
}
